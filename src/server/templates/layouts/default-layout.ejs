<!DOCTYPE html>
<html lang="<%= data.lang %>">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= helpers.tr('My Routes') %></title>
  <link rel='icon' type='image/png' href='<%= data.rootLink %>favicon.png'>
  <link rel="stylesheet" href="<%= data.rootLink %>dist/<%= data.version %>/css/main.css">
</head>
<body>
  <% if(data.content) { %>
    <%- 
      await partials[data.content]({ 
        data: data.contentData, 
        partials,  
        helpers
      }) 
    %> 
  <% } %> 
  <script src="<%= data.rootLink %>dist/<%= data.version %>/js/main.js" type="module"></script>
</body>
</html>
